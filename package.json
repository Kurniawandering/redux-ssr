{
  "name": "react-kit",
  "version": "0.1.0",
  "description": "A starter kit for SSR React-Redux",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "clean": "rm -rf __public__",
    "build": "npm run clean && ./node_modules/.bin/webpack -p --config webpack/prod.config.js --progress --profile --colors",
    "stats": "./node_modules/.bin/webpack --config webpack/prod.config.js  --profile --json > ./stats.json",
    "postinstall": "npm run build",
    "dev": "nodemon app.js",
    "start": "NODE_ENV=production PORT=8080 node app.js"
  },
  "author": "Hendrik Kurniawan",
  "license": "ISC",
  "dependencies": {
    "babel-core": "^6.3.21",
    "babel-eslint": "^5.0.0",
    "babel-loader": "^6.2.0",
    "babel-plugin-react-transform": "^2.0.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-1": "^6.5.0",
    "babel-register": "^6.3.13",
    "css-loader": "^0.23.1",
    "eslint": "^1.10.3",
    "eslint-plugin-react": "^3.13.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "http-proxy": "^1.12.0",
    "lodash": "^4.5.0",
    "node-sass": "^3.4.2",
    "normalize.css": "^3.0.3",
    "react": "^0.14.3",
    "react-dom": "^0.14.3",
    "react-hot-loader": "^1.3.0",
    "react-redux": "^4.4.0",
    "react-router": "^2.0.0",
    "react-router-redux": "^4.0.0",
    "redux": "^3.3.1",
    "redux-devtools": "^3.1.1",
    "redux-devtools-dock-monitor": "^1.1.0",
    "redux-devtools-log-monitor": "^1.0.4",
    "sass-loader": "^3.1.2",
    "serialize-javascript": "^1.1.2",
    "style-loader": "^0.13.0",
    "webpack": "^1.12.13",
    "webpack-dev-middleware": "^1.5.1",
    "webpack-dev-server": "^1.14.1"
  }
}
